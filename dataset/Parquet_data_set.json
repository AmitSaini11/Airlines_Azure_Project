{
	"name": "Parquet_data_set",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageGen2",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "Destination_DataSet"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@item().filename_sink",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(item().filename_sink,'/Date_Part=',formatDateTime(utcnow(),'yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": "raw"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}